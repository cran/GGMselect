load("sh7GRest")
itest=7
source("supLab.R")
# +++++++++++++++++++++++++++++++++++++++++++++++++
p=30
n=30
eta=0.15
dmax=5
iG = 7
iS = 9
set.seed(iG)
Gr <- simulateGraph(p,eta)
set.seed(iS*(pi/3.1415)**iG)
X <- rmvnorm(n, mean=rep(0,p), sigma=Gr$C)

K=2.5

ptm <- proc.time()

mon7GRest <- selectQE(X, dmax, K, verbose=TRUE)

cat("Elapsed time ",  proc.time()-ptm,"\n")
cat(" Comparison to the genuine output: is the test sucessful?\n")
sh7GRest <- supLab(sh7GRest)
print(all.equal(t(mon7GRest$G), sh7GRest[1,,]))
cat ("End of test ", itest, "\n")


