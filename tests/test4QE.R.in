load("sh4GRest")
itest=4
source("supLab.R")
# +++++++++++++++++++++++++++++++++++++++++++++++++
p=50
n=30
eta=0.07
dmax=3
iG = 7
iS = 9
set.seed(iG)
Gr <- simulateGraph(p,eta)
set.seed(iS*(pi/3.1415)**iG)
X <- rmvnorm(n, mean=rep(0,p), sigma=Gr$C)

K=2.5

ptm <- proc.time()

mon4GRest <- selectQE(X, dmax, K, verbose=TRUE)

cat("Elapsed time ",  proc.time()-ptm,"\n")
cat(" Comparison to the genuine output: is the test sucessful?\n")
sh4GRest <- supLab(sh4GRest)

print(all.equal(t(mon4GRest$G), sh4GRest[1,,]))
cat ("End of test ", itest, "\n")


