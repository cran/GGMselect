load("sh3GRest")
itest=3
source("supLab.R")
# +++++++++++++++++++++++++++++++++++++++++++++++++
p=30
n=30
eta=0.17
dmax=3
iG = 7
iS = 9
set.seed(iG)
Gr <- simulateGraph(p,eta)
set.seed(iS*(pi/3.1415)**iG)
X <- rmvnorm(n, mean=rep(0,p), sigma=Gr$C)

K=2.5

ptm <- proc.time()

mon3GRest <- selectQE(X, dmax, K, verbose=TRUE)

cat("Elapsed time ",  proc.time()-ptm,"\n")
cat(" Comparison to the genuine output: is the test sucessful?\n")
sh3GRest <- supLab(sh3GRest)
print(all.equal(t(mon3GRest$G), sh3GRest[1,,]))
cat ("End of test ", itest, "\n")


